{
  "name": "qlik-rest-api",
  "version": "1.3.0",
  "description": "Interact with multiple Qlik Sense REST APIs",
  "author": {
    "email": "info@informatiqal.com",
    "name": "Informatiqal @informatiqal",
    "url": "https://informatiqal.com"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "mocha --require ts-node/register test/**/*.spec.ts",
    "test-badge": "mocha --reporter mocha-badge-generator --reporter-options=badge_format=png,badge_output=badge.png --require ts-node/register test/**/*.spec.ts",
    "docs": "npx typedoc src/index.doc.ts --name \"Qlik REST API\" --excludePrivate --hideGenerator --darkHighlightTheme dark-plus",
    "build-tsc": "tsc",
    "build": "rollup -c",
    "watch": "rollup -cw"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "qlik",
    "sense",
    "rest",
    "api"
  ],
  "dependencies": {
    "axios": "^0.21.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.25",
    "chai": "^4.3.0",
    "dotenv": "^8.2.0",
    "es6-template-strings": "^2.0.1",
    "mocha": "^8.2.1",
    "mocha-badge-generator": "^0.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rollup": "^2.56.2",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.22.7",
    "typescript": "^4.3.5"
  },
  "prettier": {
    "singleQuote": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Informatiqal/qlik-rest-api.git"
  },
  "bugs": {
    "url": "https://github.com/Informatiqal/qlik-rest-api/issues"
  },
  "homepage": "https://informatiqal.com/qlik-sense-rest-api-package/"
}